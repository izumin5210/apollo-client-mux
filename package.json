{
  "name": "apollo-link-mux",
  "version": "0.0.1",
  "description": "Apollo Link for multiplexing multilpe GraphQL endpoints",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "gen": "pnpm gen:graph1 && pnpm gen:graph2",
    "gen:graph1": "graphql-codegen --project graph1",
    "gen:graph2": "graphql-codegen --project graph2",
    "lint": "biome check --apply src",
    "typecheck": "tsc -p . --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "graphql",
    "apollo",
    "apollo-client",
    "apollo-link"
  ],
  "homepage": "https://github.com/izumin5210/apollo-link-mux",
  "author": "izumin5210 <m@izum.in>",
  "license": "MIT",
  "packageManager": "pnpm@8.15.3+sha256.fc4a49bd609550a41e14d20efbce802a4b892aa4cac877322de2f0924f122991",
  "devDependencies": {
    "@apollo/client": "^3.9.5",
    "@biomejs/biome": "1.5.3",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@graphql-tools/schema": "^10.0.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tsconfig/strictest": "^2.0.3",
    "@types/node": "^20.0.0",
    "graphql": "^16.8.1",
    "graphql-config": "^5.0.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "peerDependencies": {
    "@apollo/client": "^3.8.0",
    "graphql": "^16.0.0"
  }
}
